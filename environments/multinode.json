          "placement-api": { "host": "192.168.101.60" }
        }
      },
      "bind_service": {
        "db": { "host": "192.168.101.60" },
        "mq": { "host": "192.168.101.60" },
        "main": {
          "identity": { "host": "0.0.0.0" }
        },
